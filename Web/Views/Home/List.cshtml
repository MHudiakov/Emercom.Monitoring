@model List<Web.Models.Unit.UnitModel>

<div class="panel-body">
    <div class="row">
        <div class="col-lg-12">
            <table class="table table-bordered">
                <tr>
                    <th>Подразделение</th>
                    <th>Объект</th>
                    <th>Номер</th>
                    <th>Описание/Примечание</th>
                    <th>Действие</th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.DivisionName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Number)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Description)
                        </td>
                        <td>
                            <a class="btn btn-default btn-sm pull-left" href="@Url.Action("UnitInfo", "Unit", new { id = item.Id })">
                                <span class="glyphicon glyphicon-search" aria-hidden="true"></span>Показать
                            </a>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>